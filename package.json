  "description": "Library for making NATS requests that receive multiple responses.",
  "main": "dist/requestMulti.js",
  "types": "dist/requestMulti.d.ts",
  "scripts": {
    "prepare": "npx tsc",
    "pack": "npm pack",
    "test-unit": "jest -c jest.unit.config.js ./tests/unit/",
