    "nats": "^2.1.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/await-timeout": "^0.3.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.4",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0"
